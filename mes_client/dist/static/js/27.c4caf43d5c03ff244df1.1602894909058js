    return {
      /* 视图显示控制 */
      showViewid: 'list',
      /* 列表页数据 */
      list: [],
      listCount: 0,
      listPreUrl: '',
      listNextUrl: '',
      /* 列表页查询参数 */
      selectItem: {
        create_user: '',
        searchValue: '',
        workshop: '',
        team: ''
      },
      /* 列表页数据排序 */
      ordering: '-id',
      /* 详情页数据 */
      detail: [],
      team: {},
      /* 创建页表单项数据 */
      formItem: {
        id: '',
        name: '',
        code: '',
        workshop: '',
        team: null,
        skill: [],
        job_number: '',
        post: '',
        wechat: '',
        image: [],
        file: [],
        attribute1: '',
        attribute2: '',
        attribute3: '',
        attribute4: '',
        attribute5: '',
